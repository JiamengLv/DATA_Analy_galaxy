   "outputs": [],
   "source": [
    "# 进一步研究\n",
    "\n",
    "# 1. 生成模拟数据集\n",
    "heights_data = sts.norm.rvs(loc = 1.7, scale = 0.1, size = 1001)  ## 正态分布 随机样本数据 loc是均值，scale是标准差\n"
   ],
   "metadata": {
    "collapsed": false
   }
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "outputs": [
    {
     "data": {
      "text/plain": "<Figure size 1000x800 with 1 Axes>",
      "image/png": "\n"
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# 2.计算不同数据量下的后验分布\n",
    "prior = uniform_dist\n",
    "posterior_dict = {}\n",
    "\n",
    "plt.figure(figsize = (10, 8))\n",
    "\n",
    "for ind, datum in enumerate(heights_data):\n",
    "  likelihood = likelihood_func(datum, mu)\n",
    "  unnormalized_posterior = prior * likelihood                                 # 后验分布替代先验分布\n",
    "  normalized_posterior = unnormalized_posterior/sp.integrate.trapz(unnormalized_posterior, mu)\n",
    "  prior = normalized_posterior\n",
    "  posterior_dict[ind] = normalized_posterior\n",
    "  if ind%200 == 0:\n",
    "    plt.plot(mu, normalized_posterior, label = f'Model after observing {ind} data')\n",
    "\n",
    "plt.legend()\n",
    "plt.show()"
   ],
   "metadata": {
    "collapsed": false,
    "ExecuteTime": {
     "start_time": "2023-12-19T10:54:38.269487Z",
     "end_time": "2023-12-19T10:54:38.587825Z"
    }
   }
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "outputs": [
    {
     "data": {
      "text/plain": "Text(0, 0.5, 'Predicted Mean $\\\\mu$')"
     },
     "execution_count": 22,
     "metadata": {},
     "output_type": "execute_result"
    },
    {
     "data": {
      "text/plain": "<Figure size 1200x800 with 1 Axes>",
